<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entity.User" table="user">
		<id name="uid" type="int">
			<generator class="native"/>		
		</id>
		<property name="username" type="java.lang.String"></property>
		<property name="pwd" type="java.lang.String"></property>
		<property name="email" type="java.lang.String"></property>
		<property name="city" type="java.lang.String"></property>
	</class>
</hibernate-mapping>